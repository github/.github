                                                data-ga-config="\{&#34;click&#34;: \{&#34;event&#34;: &#34;general_link_click&#34;, &#34;link_url&#34;: true, &#34;link_text&#34;: true, &#34;link_type&#34;: true, &#34;module_name&#34;: true, &#34;section_header&#34;: true\}\}"\par
